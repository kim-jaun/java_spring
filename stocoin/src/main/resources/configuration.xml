<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="board" type="com.sc.stocoin.model.Board"/>
		<typeAlias alias="member" type="com.sc.stocoin.model.Member"/>
		<typeAlias alias="admin" type="com.sc.stocoin.model.Admin"/>
		<typeAlias alias="reply" type="com.sc.stocoin.model.Reply"/>
		<typeAlias alias="replyLikes" type="com.sc.stocoin.model.ReplyLikes"/>
		<typeAlias alias="qna" type="com.sc.stocoin.model.Qna"/>
		<typeAlias alias="qnaReply" type="com.sc.stocoin.model.QnaReply"/>
		<typeAlias alias="tradeCoin" type="com.sc.stocoin.model.TradeCoin"/>
		<typeAlias alias="tradeStock" type="com.sc.stocoin.model.TradeStock"/>
		<typeAlias alias="myCoin" type="com.sc.stocoin.model.MyCoin"/>
		<typeAlias alias="myStock" type="com.sc.stocoin.model.MyStock"/>
		<typeAlias alias="favoriteCoin" type="com.sc.stocoin.model.FavoriteCoin"/>
		<typeAlias alias="favoriteStock" type="com.sc.stocoin.model.FavoriteStock"/>
		<typeAlias alias="chat" type="com.sc.stocoin.model.Chat"/>
	</typeAliases>
	<!-- 매퍼정보 설정 -->
	<mappers>
		<mapper resource="board.xml" />
		<mapper resource="member.xml" />
		<mapper resource="admin.xml" />
		<mapper resource="reply.xml" />
		<mapper resource="replyLikes.xml" />
		<mapper resource="qna.xml" />
		<mapper resource="qnaReply.xml" />
		<mapper resource="tradeCoin.xml" />
		<mapper resource="tradeStock.xml" />
		<mapper resource="myCoin.xml" />
		<mapper resource="myStock.xml" />
		<mapper resource="favoriteCoin.xml" />
		<mapper resource="favoriteStock.xml" />
		<mapper resource="chat.xml" />
	</mappers>
</configuration>