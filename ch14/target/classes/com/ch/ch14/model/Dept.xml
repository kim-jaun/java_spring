<hibernate-mapping>
	<class name="com.ch.ch14.model.Dept" table="dept">
		<id name="deptno" column="deptno" type="java.lang.Integer" />
		<property name="dname" column="dname" type="java.lang.String" />
		<property name="loc" column="loc" type="java.lang.String" />
	</class>
</hibernate-mapping>